<style>
  .word-switcher {
    text-transform: none;
    font-weight: 700;
    font-size: 151px;
    line-height: (151/140);
    letter-spacing: -(50em/1000); /* illustrator conversion */
    transition: 0.2s ease-in-out all;

    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    margin: 0;

    background: #0b213e;
  }

  .star-unpacked {
    /**
     * Fallback styles for the SVG
     */
    font-size: 107px;
    letter-spacing: -0.05em;
    line-height: 0.98;
  }

  .star-unpacked .word {
    display: none;
    color: #ffffff;
    text-shadow: -1px -1px 0 #b3b3b3, 1px -1px 0 #b3b3b3, -1px 1px 0 #b3b3b3, 1px 1px 0 #b3b3b3;
  }

  .star-unpacked .word:first-of-type {
    color: #e7942d;
    text-shadow: none;
  }

  .star-unpacked .svg-star-unpacked__ratio-wrapper {
    position: relative;
    width: 100%;
    height: 0;
    padding-bottom: 25%;
  }

  .star-unpacked .svg-star-unpacked {
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
  }

  .star-unpacked .svg-word {
    font-weight: 700;
    letter-spacing: -0.05em;
  }

  .star-unpacked .svg-word--text-stroke,
  .star-unpacked .svg-word--text-stroke-2 {
    fill: none;
    stroke: none;
    stroke-miterlimit: 10;
    stroke-dashoffset: -900;
    stroke-dasharray: 900;
    stroke-linecap: butt;
    stroke-linejoin: round;
    position: relative;
  }

  .star-unpacked .svg-word--text-stroke {
    stroke: #b3b3b3;
    opacity: 1.0;
    -webkit-transition: ease-in-out 3.4s all;
    transition: ease-in-out 3.4s all;
    z-index: 1000;
  }

  .star-unpacked .svg-word--text-stroke-2 {
    stroke: #e7942d;
    z-index: 1020;
  }

  .star-unpacked .svg-word--text-fill {
    fill: #e7942d;
    fill-opacity: 0;
  }

  .star-unpacked .svg-word-group--highlight .svg-word--text-stroke {
    stroke: #b3b3b3;
    opacity: 0.5;
  }

  .star-unpacked .svg-word-group--highlight .svg-word--text-stroke-2 {
    stroke: #e7942d;
    z-index: 1020;
  }

  .star-unpacked .svg-word--text-stroke {
    -webkit-animation: dash 2.5s ease-in-out forwards;
            animation: dash 2.5s ease-in-out forwards;
  }

  .star-unpacked .svg-word-group--highlight .svg-word--text-stroke-2 {
    -webkit-animation: dash 2s ease-in-out forwards;
            animation: dash 2s ease-in-out forwards;
  }

  .star-unpacked .svg-word-group--highlight .svg-word--text-fill {
    -webkit-animation: fill 1.5s ease-in-out forwards;
            animation: fill 1.5s ease-in-out forwards;
    -webkit-animation-delay: 1.7s;
            animation-delay: 1.7s;
  }

  .star-unpacked.word-switcher--first-run .svg-word-group--highlight .svg-word--text-stroke-2 {
    -webkit-animation: dash-de-dash 9s ease-in-out forwards;
            animation: dash-de-dash 9s ease-in-out forwards;
  }

  .star-unpacked.word-switcher--first-run .svg-word-group--highlight .svg-word--text-fill {
    -webkit-animation: fill-de-fill 2.5s ease-in-out forwards;
            animation: fill-de-fill 2.5s ease-in-out forwards;
    -webkit-animation-delay: 3.4s;
            animation-delay: 3.4s;
  }

  .star-unpacked.word-switcher--first-run .svg-word-group:nth-of-type(4).svg-word-group--highlight .svg-word--text-stroke-2 {
    -webkit-animation: dash 4.5s ease-in-out forwards;
            animation: dash 4.5s ease-in-out forwards;
  }

  .star-unpacked.word-switcher--first-run .svg-word-group:nth-of-type(4).svg-word-group--highlight .svg-word--text-fill {
    -webkit-animation: fill 1.25s ease-in-out forwards;
            animation: fill 1.25s ease-in-out forwards;
    -webkit-animation-delay: 3.4s;
            animation-delay: 3.4s;
  }

  @-webkit-keyframes dash {
    100% {
      stroke-dashoffset: 0;
    }
  }
  @keyframes dash {
    100% {
      stroke-dashoffset: 0;
    }
  }

  @-webkit-keyframes dash-de-dash {
    0% {
      stroke-dashoffset: -900;
    }
    50% {
      stroke-dashoffset: 0;
    }
    100% {
      stroke-dashoffset: -900;
    }
  }

  @keyframes dash-de-dash {
    0% {
      stroke-dashoffset: -900;
    }
    50% {
      stroke-dashoffset: 0;
    }
    100% {
      stroke-dashoffset: -900;
    }
  }

  @-webkit-keyframes fill {
    100% {
      fill-opacity: 1;
    }
  }

  @keyframes fill {
    100% {
      fill-opacity: 1;
    }
  }

  @-webkit-keyframes fill-de-fill {
    0% {
      fill-opacity: 0;
    }
    20% {
      fill-opacity: 1;
    }
    60% {
      fill-opacity: 1;
    }
    100% {
      fill-opacity: 0;
    }
  }

  @keyframes fill-de-fill {
    0% {
      fill-opacity: 0;
    }
    20% {
      fill-opacity: 1;
    }
    60% {
      fill-opacity: 1;
    }
    100% {
      fill-opacity: 0;
    }
  }


  /* Layout */

  .word-switcher {
    padding: 22% 10% 20% 10%;
    margin-bottom: 32px;
  }

  @media only screen and (min-width: 800px) {
    .word-switcher {
      padding-top: 12%;
      padding-bottom: 10%;
    }
  }

  .word-switcher .svg-star-unpacked {
    width: 100%;
  }


</style>

